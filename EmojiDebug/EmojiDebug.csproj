<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <PackageProjectUrl>https://github.com/colinkiama/EmojiDebug</PackageProjectUrl>
    <Description>Easily write debug messages with emoji.</Description>
    <RepositoryUrl>https://github.com/colinkiama/EmojiDebug</RepositoryUrl>
    <PackageLicenseFile>LICENCE.md</PackageLicenseFile>
    <PackageReleaseNotes>Fixed bug with incorrect output behaviour</PackageReleaseNotes>
    <PackageId>EmojiDebug.ColinKiama</PackageId>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENCE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
